<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--Code using for Warehouse (Stock) -->
        <record id="seq_type_kderp_warehouse_code" model="ir.sequence.type">
            <field name="name">KDERP Warehouse Code</field>
            <field name="code">kderp_stock_warehouse_code</field>
        </record>
        <record id="seq_kderp_warehouse_code" model="ir.sequence">
            <field name="name">KDERP Warehouse Code</field>
            <field name="code">kderp_stock_warehouse_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">W$</field>
            <field name="example">WH0000</field>
        </record>


        <!--Code using for Stock Packing -->
        <record id="seq_type_kderp_stock_picking_code_in" model="ir.sequence.type">
            <field name="name">KDERP Stock Picking Code</field>
            <field name="code">kderp_stock_picking_code_in</field>
        </record>
        <record id="seq_type_kderp_stock_picking_code_out" model="ir.sequence.type">
            <field name="name">KDERP Stock Picking Code</field>
            <field name="code">kderp_stock_picking_code_out</field>
        </record>
        <record id="seq_type_kderp_stock_picking_code_internal" model="ir.sequence.type">
            <field name="name">KDERP Stock Picking Code</field>
            <field name="code">kderp_stock_picking_code_int</field>
        </record>        
        
        <record id="seq_kderp_stock_picking_in_code" model="ir.sequence">
            <field name="name">KDERP Stock Picking Code - Supplier to KDVN</field>
            <field name="code">kderp_stock_picking_code_in</field>
            <field eval="5" name="padding"/>
            <field name="prefix">P$</field>
            <field name="suffix">INYY-</field>
            <field name="example">PINYY-00000</field>
        </record>
        
        <record id="seq_kderp_stock_picking_out_code" model="ir.sequence">
            <field name="name">KDERP Stock Picking Code - KDVN to Customer</field>
            <field name="code">kderp_stock_picking_code_out</field>
            <field eval="5" name="padding"/>
            <field name="prefix">P$</field>
            <field name="suffix">OUTYY-</field>
            <field name="example">PHOUTYY-00000</field>
        </record>
        
        <record id="seq_kderp_stock_picking_move_kinden_code" model="ir.sequence">
            <field name="name">KDERP Stock Picking Code - KDVN Move Internal</field>
            <field name="code">kderp_stock_picking_code_int</field>
            <field eval="5" name="padding"/>
            <field name="prefix">P$</field>
            <field name="suffix">MYY-</field>
            <field name="example">PHMYY-00000</field>
        </record>        
        
    </data>
</openerp>