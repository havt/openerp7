<openerp>
    <data>
        <!-- Move for Move Internal -->
        <record id="kderp_stock_inout.kderp_view_packing_detail_internal_tree" model="ir.ui.view">
            <field name="name">kderp.view.packing.detail.internal.tree</field>
            <field name="model">stock.move</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Packing Detail" editable="top">
                    <field colspan="4" context="{'filter_by_location_id':location_id,'location_ids':location_id,'tree_view_ref':'kderp_stock_inout.view_kderp_product_for_stock_tree'}" name="product_id" domain="[('type','!=','service')]" on_change="onchange_product_id(product_id,location_id,location_dest_id)" required="1" select="1"/>
                    <field name="product_qty" select="1" string="Qty."/>
                    <field name="product_uom" select="1" string="Unit"/>
                    <field name="location_id"/>
                    <field name="from_analytic_id" context="{'filter_by_location':location_id}"/>
                    <field name="location_dest_id"/>
                    <field name="to_analytic_id" context="{'filter_by_location':location_dest_id}"/>
                    <field name="date_expected"/>
                    <field name="state" string="State"/>
                </tree>
            </field>
        </record>

    </data>
</openerp>